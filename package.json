{
  "name": "remark-redactable",
  "version": "0.3.0",
  "description": "remark plugin to enable other plugins for redaction and restoration of content",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --ext .js src/ test/",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "tape ./test/unit/*.test.js",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-dot-org/remark-redactable.git"
  },
  "author": "Elijah Hamovitz (elijah@code.org)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/code-dot-org/remark-redactable/issues"
  },
  "homepage": "https://github.com/code-dot-org/remark-redactable#readme",
  "devDependencies": {
    "@code-dot-org/remark-plugins": "code-dot-org/remark-plugins#add-link-plugin",
    "eslint": "^5.9.0",
    "remark": "^10.0.1",
    "tape": "^4.9.1"
  }
}
